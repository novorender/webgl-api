{
    "name": "webgl-api",
    "version": "1.0.0",
    "description": "A Web API for scalable 3D rendering in the cloud.",
    "scripts": {
        "start": "webpack serve --mode development",
        "build:schema": "typescript-json-schema tsconfig.json RenderState.Scene --include src/webgl-api/*.ts >render-state-schema.json",
        "generate_binary": "node src/node/generateBinary.mjs",
        "test": "ts-node --esm src/test/index.ts"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/novorender/webgl-api.git"
    },
    "keywords": [
        "3d",
        "webgl",
        "cad",
        "gis"
    ],
    "author": "Tore Lode",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/novorender/webgl-api/issues"
    },
    "homepage": "https://github.com/novorender/webgl-api#readme",
    "devDependencies": {
        "jimp": "^0.16.1",
        "resolve-typescript-plugin": "^1.2.0",
        "ts-loader": "9.2.8",
        "ts-node": "^10.7.0",
        "tslib": "2.4.0",
        "typescript": "4.6.3",
        "typescript-json-schema": "0.53.0",
        "webpack": "5.72.0",
        "webpack-cli": "4.9.2",
        "webpack-dev-server": "4.8.1",
        "webpack-glsl-loader": "1.0.1"
    },
    "dependencies": {
        "gl-matrix": "3.4.3"
    }
}
